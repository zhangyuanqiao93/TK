<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>

    <declare-styleable name="PullToRefreshFrameLayout">
        <attr name="has_header" format="boolean" />
        <attr name="has_footer" format="boolean" />
        <attr name="has_shadow" format="boolean" />
        <attr name="has_divider" format="boolean" />
    </declare-styleable>
</resources>